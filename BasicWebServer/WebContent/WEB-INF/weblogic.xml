<!DOCTYPE weblogic-web-app PUBLIC "-//BEA Systems, Inc.//DTD Web Application 8.1//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-web-jar.dtd">
<weblogic-web-app>  
	<wls:weblogic-version>10.3</wls:weblogic-version>
    <wls:context-root>BasicWebServer</wls:context-root>
     
    <container-descriptor>
    <index-directory-enabled>true</index-directory-enabled>
  </container-descriptor>

  <virtual-directory-mapping>
    <local-path>/nscorphtml</local-path>
    <url-pattern>*</url-pattern>
  </virtual-directory-mapping>

  <context-root>/nscorphtml</context-root>
    
</weblogic-web-app>